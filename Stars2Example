/**
 *
 * @author jennyhuebner
 */
public class Stars2 {
   public static void main (String[] args){
        for (int line = 0; line < 10; line++) {
            for (int star = 0; star < line; star++)
                System.out.print(" ");
            for (int star = line; star < 10; star++)
                System.out.print("*");
             System.out.println();
    }
 }
 }
